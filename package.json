{
  "name": "opra",
  "description": "Single page app builder",
  "main": "./lib/opra.js",
  "bin": {
    "opra": "./bin/opra"
  },
  "version": "0.12.4",
  "author": "Jakob Mattsson <jakob.mattsson@gmail.com> (jakobmattsson.se)",
  "repository": {
    "type": "git",
    "url": "git://github.com/jakobmattsson/opra.git"
  },
  "engines": {
    "node": ">=0.6.0"
  },
  "scripts": {
    "prepublish": "coffee -c -o lib src",
    "pretest": "coffee -c -o lib src",
    "test": "mocha --globals exists --timeout 10000 --require spec/lib.js spec/**/*.coffee",
    "coverage": "jscov src .cov && JSCOV=.cov mocha --globals exists --timeout 10000 --require spec/lib.js --compilers coffee:coffee-script --reporter mocha-term-cov-reporter spec/**/*.coffee"
  },
  "devDependencies": {
    "jscov": "0.2.0",
    "mocha-term-cov-reporter": "0.1.0",
    "should": "1.2.1",
    "sugar": "1.3.8",
    "mocha": "1.8.1"
  },
  "dependencies": {
    "underscore.string": "2.3.1",
    "less": "1.3.3",
    "underscore": "1.4.3",
    "glob": "3.1.14",
    "async": "0.1.22",
    "powerfs": "0.2.3",
    "optimist": "0.3.5",
    "browserify": "1.17.2",
    "npm": "1.2.0",
    "uglify-js": "1.2.5",
    "clean-css": "0.9.1",
    "connect": "2.7.2",
    "express-jit-coffee": "0.1.10",
    "coffee-script": "1.4.0"
  }
}
