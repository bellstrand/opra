{
  "name": "opra",
  "description": "Single page app builder",
  "main": "./src/opra.js",
  "bin": {
    "opra": "./bin/opra"
  },
  "version": "0.11.0",
  "author": "Jakob Mattsson <jakob.mattsson@gmail.com> (jakobmattsson.se)",
  "repository": {
    "type": "git",
    "url": "git://github.com/jakobmattsson/opra.git"
  },
  "engines": {
    "node": ">=0.6.0"
  },
  "devDependencies": {
    "should": "0.6.0",
    "evil": "0.1.4",
    "sugar": "1.2.2"
  },
  "scripts": {
    "prepublish": "coffee -c -o lib src",
    "pretest": "coffee -c -o lib src",
    "test": "mocha --globals exists --timeout 10000 --require spec/lib.js spec/**/*.coffee",
    "coverage": "scripts/coverage.sh 'spec/**/*.coffee' all.html && scripts/coverage.sh 'spec/unit/*.coffee' unit.html"
  },
  "dependencies": {
    "underscore.string": "2.0.0",
    "less": "1.3.0",
    "underscore": "1.3.1",
    "glob": "3.1.6",
    "async": "0.1.15",
    "powerfs": "0.1.0",
    "optimist": "0.3.4",
    "browserify": "1.10.6",
    "npm": "1.1.5",
    "uglify-js": "1.2.5",
    "clean-css": "0.3.2",
    "connect": "2.0.2",
    "express-jit-coffee": "0.1.10",
    "connect-rewrite": "0.0.1",
    "coffee-script": "1.2.0"
  }
}
