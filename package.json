{
  "name": "opra",
  "description": "Single page app builder",
  "main": "./lib/opra.js",
  "bin": {
    "opra": "./bin/opra"
  },
  "version": "0.15.0",
  "author": "Jakob Mattsson <jakob.mattsson@gmail.com> (jakobmattsson.se)",
  "repository": {
    "type": "git",
    "url": "git://github.com/jakobmattsson/opra.git"
  },
  "engines": {
    "node": ">= 0.6.0 < 0.12"
  },
  "scripts": {
    "prepublish": "coffee -c -o lib src",
    "pretest": "coffee -c -o lib src",
    "test": "mocha --globals exists --timeout 10000 --require spec/lib.js spec/**/*.coffee",
    "coverage": "jscov src .cov && JSCOV=.cov mocha --globals exists --timeout 10000 --require spec/lib.js --compilers coffee:coffee-script --reporter mocha-term-cov-reporter spec/**/*.coffee"
  },
  "devDependencies": {
    "coffee-script": ">= 1.3.0 < 2",
    "less": ">= 1.3.3 < 2",
    "jscov": ">= 0.5.0 < 0.6",
    "mocha-term-cov-reporter": ">= 0.1.0 < 0.2",
    "should": ">= 1.2.2 < 2",
    "mocha": ">= 1.8.2 < 2"
  },
  "dependencies": {
    "underscore.string": ">= 2.0.0 < 3",
    "wrench": ">= 1.4.4 < 2",
    "underscore": ">= 1.4.0 < 2",
    "glob": ">= 3.1.13 < 4",
    "browserify": ">= 1.17.0 < 2",
    "uglify-js": ">= 1.10 < 3",
    "connect": ">= 2.7.3 < 3",
    "clean-css": ">= 1.0.0 < 2",
    "async": ">= 0.1.0 < 0.3",
    "powerfs": ">= 0.2.0 < 0.3",
    "optimist": ">= 0.6.0 < 0.7",
    "express-jit-coffee": ">= 0.1.2 < 0.2",
    "npm": ">= 1.2.19 < 2"
  }
}
